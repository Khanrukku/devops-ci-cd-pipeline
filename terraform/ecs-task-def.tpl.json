{
  "family": "devops-cicd-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "REPLACE_EXECUTION_ROLE_ARN",
  "taskRoleArn": "REPLACE_TASK_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "devops-cicd-container",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/devops-cicd",
          "awslogs-region": "REPLACE_AWS_REGION",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
